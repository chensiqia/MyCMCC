<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cmccpoc.activity.AllMemberActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center">
        <TextView
            android:id="@+id/all_member_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:text="@string/all_channel_member_title_text"
            android:gravity="center"
            android:textSize="24sp"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="28px" >
        <ListView
            android:id="@+id/all_member_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@drawable/divider_line"/>
    </LinearLayout>

    <include layout="@layout/include_bottom_footer" />

